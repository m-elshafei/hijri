{
    "name": "elshafei/HijriDate",
    "description": "Laravel package to convert Gregorian dates to Hijri dates",
    "license": "MIT",
    "authors": [
        {
            "name": "Moustafa Elshafei",
            "email": "moustafaelshafhy@gmail.com"
        }
    ],
    "keywords": [
        "m-elshafei",
        "laravel",
        "hijri-date",
        "hijri",
        "convert-date-to-hijri",
        "artisan"
    ],
    "homepage": "https://github.com/m-elshafei/hijri-date",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Elshafei\\LaravelHijriDate\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/m-elshafei/hijri-date"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "elshafei\\HijriDate\\LaravelHijriDateServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "11.0"
    }
}
